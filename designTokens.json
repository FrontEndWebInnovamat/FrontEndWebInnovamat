  "alias": {
    "color": {
      "alias": {
        "cm": {
          "bg": {
            "bg-accent-inverted": {
              "$type": "color",
              "$value": "{color.global.teal.teal500}",
              "$description": "Use for the background color of actions elements when status of accent or active. Components such as buttons or toggle switch."
            },
            "bg-accent": {
              "$type": "color",
              "$value": "{color.global.teal.teal100}",
              "$description": "Use as a subtle background of active elements. Normaly, it is accompained by a harder accent shade in icons or text."
            },
            "bg-neutral-inverted": {
              "$type": "color",
              "$value": "{color.global.neutral.neutral700}",
              "$description": "Use for active elements thant don't use accent color. For example checkbox when solutions are not active or tooltips."
            },
            "bg-neutral-inverted-subtle": {
              "$type": "color",
              "$value": "{color.global.neutral.neutral400}",
              "$description": "Use as a inverted background for small elements that don't use surface tokens."
            },
            "bg-neutral-subtle": {
              "$type": "color",
              "$value": "{color.global.neutral.neutral100}",
              "$description": "Use as a background for small elements that don't use surface tokens. For example the secondary button background."
            },
            "bg-disabled": {
              "$type": "color",
              "$value": "rgba(0,0,0,0.05)",
              "$description": "Use for background of disabled elements."
            },
            "bg-error": {
              "$type": "color",
              "$value": "{color.global.red.red100}",
              "$description": "Semantic. Use for background of error elements."
            },
            "bg-success": {
              "$type": "color",
              "$value": "{color.global.green.green100}",
              "$description": "Semantic. Use for background of success elements."
            },
            "bg-warning": {
